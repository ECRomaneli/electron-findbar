{
  "name": "electron-findbar",
  "version": "0.0.1-debug",
  "description": "Chrome-like findbar for your Electron app.",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    },
    "./remote": {
      "types": "./dist/remote.d.ts",
      "default": "./dist/remote.js"
    }
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "!dist/preload.d.ts"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && webpack",
    "build:dev": "npm run clean && NODE_ENV=development webpack",
    "dev": "webpack --mode development --watch",
    "sample": "electron --trace-warnings test/sample.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ECRomaneli/electron-findbar.git"
  },
  "keywords": [
    "find",
    "findbar",
    "find-bar",
    "find bar",
    "electron-find",
    "electron-findbar",
    "electron-find-bar",
    "electron",
    "chrome-find",
    "chrome-findbar",
    "chrome-find-bar",
    "search",
    "search-bar"
  ],
  "author": "ECRomaneli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ECRomaneli/electron-findbar/issues"
  },
  "homepage": "https://github.com/ECRomaneli/electron-findbar#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@types/electron": "^1.6.12",
    "@types/node": "^25.1.0",
    "clean-css": "^5.3.3",
    "css-loader": "^7.1.3",
    "cssnano": "^7.1.2",
    "electron": ">=12.0.0",
    "html-webpack-plugin": "^5.6.6",
    "mini-css-extract-plugin": "^2.10.0",
    "postcss-loader": "^8.2.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  }
}
